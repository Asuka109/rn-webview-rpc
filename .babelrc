{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 versions", "ios >= 7", "android >= 4.4"
        ]
      }
    }],
    "react", "babel-preset-stage-3"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}